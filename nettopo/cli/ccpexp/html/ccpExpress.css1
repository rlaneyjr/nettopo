/*thermometer*/
/* Thermometer column and text */
 .thermometer {
   margin: 50% 0 0 50%;
left: -15px;
top: -100px;
width: 22px;
height: 140px;
display: block;
font:14px/152px helvetica, arial, sans-serif;
text-indent: 36px;

border-radius: 22px 22px 0 0;
border: 2px solid #DAD7D5;
border-bottom: none;
position: relative;
box-shadow: inset 0 0 0 4px #fff;
color: #4a1c03;
background: -webkit-linear-gradient(top, #fff 0%, #fff 100%, #D84208 50%, #db3f02 100%);
    background: linear-gradient(top, #fff 0%, #fff 100%, #db3f02 50%, #db3f02 100%);
    background: -moz-linear-gradient(top, #fff 0%, #fff 100%, #db3f02 50%, #db3f02 100%);
    background: -ms-linear-gradient(top, #fff 0%, #fff 100%, #db3f02 50%, #db3f02 100%);
     background: -o-linear-gradient(top, #fff 0%, #fff 100%, #db3f02 50%, #db3f02 100%);
   
}

/* Thermometer Bulb */
 .thermometer:before {
    content: ' ';
width: 44px;
height: 44px;
display: block;
position: absolute;
top: 136px;
left: -13px;
z-index: 2;
background: #db3f02;
border-radius: 44px;
border: 2px solid #DAD7D5;
box-shadow: inset 0 0 0 4px #fff;
}
/* This piece here connects the column with the bulb */
 .thermometer:after {
  content: ' ';
width: 14px;
height: 7px;
display: block;
position: absolute;
top: 136px;
background: #db3f02;
z-index: 4;
border-left: 4px solid #fff;
border-right: 4px solid #fff;
}

